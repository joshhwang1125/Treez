<header class="menunav">
  <div class="inner">
    <h1 id="title"><%= @license.DCT + " " + @license.DCS %></h1>
    <nav class='navigation'>
      <ul>
        <%= link_to 'Delete License', license_url(@license),
    data: { confirm: 'Are you sure?' }, method: :delete %>
        <%= link_to "Back", licenses_url %>
      </ul>
    </nav>
  </div>
</header>



<table>
  <% License::COLUMNS.each_with_index do |attr_name, i| %>
    <tr>
      <td><%= License::DISPLAY[i] %></td>
      <td><%= @license[attr_name] %></td>
    </tr>
  <% end %>
</table>

<br><br>



